{
  "name": "@kimuntupro/ai-core",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./assistants": {
      "types": "./dist/assistants/index.d.ts",
      "import": "./dist/assistants/index.js"
    },
    "./orchestration/planner": {
      "types": "./dist/orchestration/planner.d.ts",
      "import": "./dist/orchestration/planner.js"
    },
    "./orchestration/executor": {
      "types": "./dist/orchestration/executor.d.ts",
      "import": "./dist/orchestration/executor.js"
    },
    "./orchestration/middleware": {
      "types": "./dist/orchestration/middleware.d.ts",
      "import": "./dist/orchestration/middleware.js"
    },
    "./logging": {
      "types": "./dist/logging/logger.d.ts",
      "import": "./dist/logging/logger.js"
    },
    "./usage": {
      "types": "./dist/usage/quota.d.ts",
      "import": "./dist/usage/quota.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "openai": "^4.67.0",
    "tavily": "^1.0.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/ui": "^2.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}
